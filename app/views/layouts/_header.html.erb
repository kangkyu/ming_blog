<h1 class="text-center">Ming Rocks!</h1>

<div class="text-center">
<% if current_user %>
  <%= link_to "Log Out", session_path, method: :delete, class: "btn btn-link" %>
<% else %>
  <%= link_to "login", login_path, class: "btn btn-link" %>
  <%= link_to "signup", signup_path, class: "btn btn-link"  %>
<% end %>
</div>
